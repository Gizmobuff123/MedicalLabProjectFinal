package Interfaces;
import java.awt.HeadlessException;
import javax.swing.*;
import Interfaces.NewObjection;

public class NewObjection extends JFrame
{
	public JButton GoBackToObjection =new JButton();
	public JLabel Objectionid=new JLabel();
	public JLabel Customerid= new JLabel();
	public JLabel Discription=new JLabel();
	
	
	public JTextField Objectionidtext=new JTextField();
	public JTextField Customeridtext=new JTextField();
	public JTextField Discriptiontext=new JTextField();
	
	
	JButton AddObjection=new JButton();
	JButton Show=new JButton();
	
	
	public NewObjection() throws HeadlessException 
	{
		
		super();
		interface19();
	}
	
public void interface19()
{
	GoBackToObjection.setText("Back");
	GoBackToObjection.setBounds(30,30,70,45);

    Objectionid.setText("Objection id");
    Objectionid.setBounds(50,100,100,55);
    Objectionidtext.setBounds(175,100,100,55);

    Customerid.setText("Customer id");
    Customerid.setBounds(50,200,100,55);
    Customeridtext.setBounds(175,200,100,55);
    
    Discription.setText("Description id");
    Discription.setBounds(50, 300, 100, 55);
    Discriptiontext.setBounds(175,300,100,55);
    
    AddObjection.setText("Ok");
	AddObjection.setBounds(100,400,85,55);
	
	Show.setText("Show");
	Show.setBounds(185,400,75,55);
	this.setSize(800,800);
    
    this.show();
	this.setLayout(null);
	this.add(GoBackToObjection);
	this.add(Objectionid);
	this.add(Customerid);
	this.add(Discription);
	this.add(Objectionidtext);
	this.add(Customeridtext);
	this.add(Discriptiontext);
	this.add(AddObjection);
	this.add(Show);

}
	
	public static void main(String[] args) 
	{
		NewObjection ui=new NewObjection();
	}	
    		
}
